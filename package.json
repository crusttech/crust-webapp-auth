{
  "name": "crust-webapp-auth",
  "version": "2019.12.0",
  "private": true,
  "description": "Crust Authentication WebApp",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build --mode production",
    "lint": "vue-cli-service lint",
    "test:unit": "vue-cli-service test:unit",
    "cdeps": "yarn upgrade corteza-webapp-common corteza-webapp-auth"
  },
  "main": "index.js",
  "dependencies": {
    "corteza-webapp-auth": "cortezaproject/corteza-webapp-auth.git#2019.12.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.5.0",
    "@vue/cli-plugin-eslint": "^3.5.0",
    "@vue/cli-plugin-unit-mocha": "^3.8.0",
    "@vue/cli-service": "^3.5.0",
    "@vue/test-utils": "1.0.0-beta.29",
    "babel-eslint": "^10.0.1",
    "chai": "^4.1.2",
    "eslint": "^5.8.0",
    "eslint-plugin-vue": "^5.0.0",
    "node-sass": "^4.13",
    "sass-loader": "^7.1.0",
    "vue-template-compiler": "^2.5.21"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "contributors": [
    "Denis Arh <denis.arh@gmail.com>"
  ],
  "gitHooks": {
    "pre-commit": "yarn lint"
  },
  "license": "Apache-2.0",
  "stylelint": {
    "plugins": [
      "stylelint-scss"
    ],
    "extends": "stylelint-config-standard",
    "rules": {
      "color-hex-case": null,
      "color-hex-length": null,
      "no-empty-source": null,
      "selector-list-comma-newline-after": null
    }
  }
}
